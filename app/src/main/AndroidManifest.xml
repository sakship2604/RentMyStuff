<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inferno.rentmystuff">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".RenterRegister" />
        <activity android:name=".UserRegister" />
        <activity android:name=".RenterHome" />
        <activity android:name=".RenterProfile" />
        <activity android:name=".RenterUpdateProfile" />
        <activity android:name=".UserHome" />
        <activity android:name=".AddItem" />
        <activity android:name=".UserProfile" />
        <activity android:name=".UserUpdateProfile" />
        <activity android:name=".SeeProducts" />
        <activity android:name=".CustomSeeProduct" />
        <activity android:name=".SelectProduct" />
        <activity android:name=".UserItemDetails" />
        <activity android:name=".UserMail" />
        <activity android:name=".UserMessage"></activity>
    </application>

</manifest>